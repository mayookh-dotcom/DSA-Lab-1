#include <stdio.h>

int main(){
    int n, i,j,count=0;
    int arr[50];

    printf("Enter the number of elements: ");
    scanf("%d", &n);
   
    printf("Enter %d elements :", n);
    for(i = 0; i < n; i++){
        scanf("%d", &arr[i]);
    }
    
    for(i = 0; i < n; i++){
        for(j = i + 1; j < n; j++){
            if(arr[i] == arr[j]){
                count++;
            }
        }
    }
    printf("--------------------------\n");
    printf("Count of duplicate elements is: %d\n", count);
    return 0;
}
